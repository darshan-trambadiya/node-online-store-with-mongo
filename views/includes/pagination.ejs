<div class="col-12">
    <!-- Pagination -->
    <div class="pagination text-center">
        <ul class="pagination-list">
            <% if (currentPage !==1 && previousPage !==1) { %>
                <li><a href="?page=1">1</a></li>
                <% } %>
                    <% if (hasPreviousPage) { %>
                        <li><a href="?page=<%= previousPage %>">
                                <%= previousPage %>
                            </a></li>
                        <% } %>
                            <li class="active"><a href="?page=<%= currentPage %>">
                                    <%= currentPage %>
                                </a></li>
                            <% if (hasNextPage) { %>
                                <li><a href="?page=<%= nextPage %>">
                                        <%= nextPage %>
                                    </a></li>
                                <% } %>
                                    <% if (lastPage !==currentPage && nextPage !==lastPage) { %>
                                        <li><a href="?page=<%= lastPage %>">
                                                <%= lastPage %>
                                            </a></li>
                                        <% } %>
        </ul>
    </div>
    <!--/ End Pagination -->
</div>